<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <changeSet id="2" author="mikhail">
        <createTable tableName="price">
            <column name="id" type="int" autoIncrement="true">
                <constraints primaryKey="true" primaryKeyName="pk_priceId" nullable="false"/>
            </column>
            <column name="lashesId" type="int">
                <constraints referencedTableName="lashes" referencedColumnNames="id" foreignKeyName="fk_price_lashesId" nullable="false"/>
            </column>
            <column name="cost" type="real">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>